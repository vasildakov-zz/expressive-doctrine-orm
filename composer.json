{
    "name": "vasildakov/expressive-doctrine",
    "description": "Doctrine for Zend-Expressive",
    "keywords": ["doctrine", "orm", "zf2", "zend-expressive"],
    "homepage": "https://github.com/vasildakov/expressive-doctrine-orm",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/orm": "^2.5",
        "container-interop/container-interop": "^1.1",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stdlib": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "squizlabs/php_codesniffer": "^2.6",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "VasilDakov\\Doctrine\\": "src/Doctrine"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilDakov\\Doctrine\\": "test/Doctrine"
        }
    }
}
